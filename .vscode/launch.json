    "version": "0.2.0",
    "configurations": [
        {
            "type": "java",
            "name": "Debug (Launch) - Current File",
            "request": "launch",
            "mainClass": "${file}"
        },
        {
            "type": "java",
            "name": "Debug (Launch)-Practice",
            "request": "launch",
            "mainClass": "Practice",
            "console": "integratedTerminal"
        },
        {
            "type": "java",
            "name": "Spring Boot-DemoApplication<project1>",
            "request": "launch",
            "cwd": "${workspaceFolder}",
            "console": "internalConsole",
            "mainClass": "com.learning.project1.DemoApplication",
            "projectName": "project1",
            "args": ""
        }
    ]
}